{
  "name": "your-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:web": "next dev",
    "dev:ai": "cd services/ai-orchestrator && python quick_start.py && uvicorn service:app --reload --port 5051",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:ai\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=.*\\.integration\\.test\\.(ts|tsx)$",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:all": "npm run typecheck && npm run lint && npm run test:unit && npm run test:integration",
    "test:smoke": "echo 'Smoke tests: checking critical paths'",
    "validate": "npm run lint && npm run typecheck && npm run test:coverage",
    "validate:quick": "npm run typecheck && npm run lint && npm run format:check && npm run test:unit",
    "validate:full": "npm run validate:quick && npm run test:integration && npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security:scan": "npm audit",
    "security:audit": "node scripts/security-audit.js",
    "security:compliance": "node scripts/compliance-check.js",
    "security:update-agents": "node scripts/update-agent-security.js",
    "security:check": "npm run security:audit && npm run security:compliance",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "plan": "echo 'ðŸ“‹ Use /plan command in Claude to outline changes'",
    "deploy:staging": "echo 'Deploy to staging'",
    "deploy:production": "echo 'Deploy to production'",
    "deploy:rollback": "echo 'Rollback deployment'",
    "feature:enable": "echo 'Enable feature flag'",
    "feature:disable": "echo 'Disable feature flag'",
    "feature:status": "echo 'Check feature flag status'",
    "monitor:errors": "echo 'Monitor errors'",
    "monitor:performance": "echo 'Monitor performance'",
    "health:check": "node scripts/health-check.js",
    "health:quick": "node scripts/quick-health.js",
    "health:api": "curl -s http://localhost:3000/api/health/check | jq",
    "health:report": "node scripts/health-check.js > docs/08_health/latest.json",
    "doctor": "node scripts/project-doctor.mjs",
    "pathway": "node scripts/pathway-cli.mjs",
    "start:cli": "node scripts/pathway-cli.mjs",
    "env:doctor": "node scripts/env-doctor.js",
    "env:validate": "node scripts/env-doctor.js",
    "progress:check": "echo 'Check implementation status with evidence'",
    "release": "npm run validate:full && git tag -a v$(node -p \"require('./package.json').version\") -m \"Release v$(node -p \"require('./package.json').version\")\"",
    "release:check": "./scripts/release-ready.sh || .\\scripts\\release-ready.ps1"
  },
  "dependencies": {
    "next": "14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-dropzone": "^14.2.3",
    "lucide-react": "^0.309.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@playwright/test": "^1.40.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}